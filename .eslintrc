{
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "document": true,
    "window": true,
    "it": true,
    "navigator": true,
    "fetch": true,
  },
  "extends": [
    "strongloop",
    "plugin:react/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-uses-vars": "error",
    "react/jsx-uses-react": "error",
    "max-len": [2, 120, 2, {"ignoreUrls": true}],
    "handle-callback-err": "warn",
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 1 }],
    "react/prop-types": "off"
  }
}
